<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/bg_color">
    <ImageView android:src="@drawable/fengexian" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="center_vertical" android:id="@+id/privacy_contact_menu_top_linear">
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_add" android:gravity="center">
			<ImageView android:src="@drawable/red_privacy_contact_menu_add" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_add"></TextView>
		</LinearLayout>
		<ImageView android:layout_width="1dp" android:layout_height="50dp" android:background="@color/hint_color"></ImageView>
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_import" android:gravity="center">
			<ImageView android:src="@drawable/red_privacy_contact_menu_import" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_import"></TextView>
		</LinearLayout>
		<ImageView android:layout_width="1dp" android:layout_height="50dp" android:background="@color/hint_color"></ImageView>
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_delete_select" android:gravity="center">
			<ImageView android:src="@drawable/red_privacy_contact_menu_pldel" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_delete_select"></TextView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/privacy_contact_menu_bottom_linear">
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_select_all" android:gravity="center">
			<ImageView android:src="@drawable/red_privacy_contact_menu_select" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_select_all"></TextView>
		</LinearLayout>
		<ImageView android:layout_width="1dp" android:layout_height="50dp" android:background="@color/hint_color"></ImageView>
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_unselect_all" android:gravity="center">
			<ImageView android:src="@drawable/red_privacy_contact_menu_unselect" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_unselect_all"></TextView>
		</LinearLayout>
		<ImageView android:layout_width="1dp" android:layout_height="50dp" android:background="@color/hint_color"></ImageView>
		<LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent"
			android:orientation="vertical" android:layout_weight="1" android:id="@+id/privacy_contact_menu_delete_action" android:gravity="center">
			<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/red_privacy_contact_menu_delete"></ImageView>
			<TextView android:textColor="@color/light_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/privacy_contact_menu_delete_action"></TextView>
		</LinearLayout>
	</LinearLayout>

</LinearLayout>
